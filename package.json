  "bugs": {
    "url": "https://github.com/smockle/tap-teamcity/issues"
  },
  "homepage": "https://github.com/smockle/tap-teamcity#readme",
  "dependencies": {
    "duplexer": "^0.1.1",
    "snyk": "^1.15.0",
    "tap-out": "^1.4.1",
    "through2": "^2.0.1"
  },
  "bin": {
    "tap-teamcity": "bin/cmd.js"
  },
  "snyk": true,
  "devDependencies": {
    "codeclimate-test-reporter": "^0.3.1",
